{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "uniqueItems": true,
  "items": {
    "type": "object",
    "properties": {
      "_id": {
        "type": "string",
        "format": "uuid"
      },
      "kitId": {
        "type": "object",
        "properties": {
          "%share": {
            "type": "string"
          }
        },
        "required": [
          "%share"
        ]
      },
      "yearOfBirth": {
        "type": "object",
        "properties": {
          "%share": {
            "type": "number"
          }
        },
        "required": [
          "%share"
        ]
      },
      "sexAssignedAtBirth": {
        "type": "object",
        "properties": {
          "%share": {
            "type": "string"
          }
        },
        "required": [
          "%share"
        ]
      },
      "ethnicity": {
        "type": "object",
        "properties": {
          "%share": {
            "type": "string"
          }
        },
        "required": [
          "%share"
        ]
      },
      "submittedAt": {
        "type": "string",
        "format": "date-time"
      }
    },
    "required": [
      "_id",
      "kitId",
      "submittedAt"
    ]
  }
}
